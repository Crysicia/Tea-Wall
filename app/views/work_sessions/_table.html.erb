<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Nom</th>
      <th>Vue</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%@work_sessions.each do |work_session|%>
    <tr>
      <td><%=work_session.date.strftime("%d %b %Y at %l:%M")%></td>
      <td><%=work_session.title%></td>
      <td><%= link_to "voir", work_session_path(work_session.id) %></td>
      <td><%= link_to 'duplicate', work_session_duplicate_path(work_session) %></td>
    </tr>
    <%end%>
  </tbody>
</table>
